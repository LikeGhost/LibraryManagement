<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="jmu.yaw.library.mapper.LibraryMapper">

    <select id="queryLibraryByLName" resultType="jmu.yaw.library.po.Library">
        select lId,lName from library where lName=#{lName};
    </select>

</mapper>